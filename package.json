{
  "name": "vape-cat",
  "version": "0.0.0",
  "main": "dist/ngapp/index.js",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "electron": "wait-on http-get://localhost:4200/ && electron .",
    "prestart": "npm run clean",
    "start": "concurrently -k \"ng serve\" \"npm run electron\"",
    "build": "ng build",
    "clean": "rimraf dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "npm run build -- --base-href ./ --prod",
    "release": "electron-builder"
  },
  "build": {
    "appId": "de.mensch0mat.VapeCat",
    "productName": "VapeCat",
    "win": {
      "target": "portable"
    },
    "files": [
      "dist/ngapp"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular/animations": "~10.0.0",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "custom-electron-titlebar": "^3.2.5",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "sqlite3": "^5.0.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.29",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "wait-on": "^5.2.0"
  }
}
